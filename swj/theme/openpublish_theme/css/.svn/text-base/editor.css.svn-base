.callout p{
	font-size: 12px;
/*	margin-bottom: 0px;
	padding-bottom:10px;*/

}
.callout {
font-size: 12px;
border: 2px solid #022444;
width: 200px;
padding: 10px;
margin: 10px;
background: #ebebeb;
float:right;
} 
.callout.left{
float:left;
}

hr {
    border: 1px solid gray;
    height: 1px;
    margin-bottom: 3px;
    margin-top: -8px;
}


table {
    border-collapse: collapse;
	margin: 10px auto;
}
td ,th {
    font-size: 14px;
    padding: 5px;
}

thead th {
	text-align:left;
    font-family: Verdana,Helvetica,Arial,sans-serif;
}

img {
	margin:5px;
}

h1.section-break{
	border-bottom : 1px solid #888888;
}

h1.page-title{
	color: #760c29;
	text-align: center;
}

.cke_panel_listItem div {float:none;font-weight:normal;padding:0;margin:0;width:auto;}

/******To hide call-out options from the styles drop-down******/
.cke_panel_listItem .callout {display:none}
.cke_panel_listItem a,
.cke_panel_listItem li
{padding:0;line-height:normal}
.cke_panel_listItem a {border:0 none;}
/**********/
sup, sub {
padding:3px;
line-height:0;
font-size:x-small;
}
sub {
vertical-align: -2px;
}
sup {
vertical-align: 2px;
}

/****table/image caption****/
/*table.image{width:auto !important;}*/
table.image,
table.image td
{border:1px dotted;padding:0 !important;font-style: italic;font-size: 12px !important;}
table.image img{margin:0}

table.caption{border:0px none; }
table.caption tr:last-child td,
table.caption .caption-row td
{ border:1px dotted;border-top-style:solid;font-style: italic;text-align:left; font-size: 12px !important;}
/************/

/******P no-spacing******/
p.no-spacing{margin:0px;}

