blockquote {
	background: none repeat scroll 0 0 #EFEFEF;
    border-left: 1px solid;
    font-size: 14px;
    margin: 10px;
    padding: 10px;
    position: relative
}

blockquote p{
	/*margin-bottom: 0px;
	padding-bottom:10px;*/
}

em {
  padding: 0 3px 0 0;
}

/*hr {
  background: #7f7f7f;
  border: none;
  height: 1px;
  margin: 0;
